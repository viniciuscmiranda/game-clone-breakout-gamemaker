            <string>///Move
//Reset
if(global.pause){
    speed=0;
} else{
    
    if(!global.playing){
        spd=start_spd;
        maxspd=start_maxspd;
        friction=start_friction;
    }
    
    //Get Direction
    xaxis=keyboard_check(rkey)-keyboard_check(lkey);
    hspeed+=(spd/10)*xaxis;
    
    //Clamp Speeds
    speed=clamp(speed,0,maxspd);
    spd=clamp(spd,start_spd,maxspd);
    maxspd=clamp(maxspd,start_maxspd,max_maxspd);
    friction=clamp(friction,start_friction,max_friction);
    
    //Go Away from the walls
    if(x+sprite_width/2&gt;rwall) hspeed-=min(spd,2);
    if(x-sprite_width/2&lt;lwall) hspeed+=min(spd,2);
}
